<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.wilfredshen.hnu.test.mapper.ScoreMapper">

    <select id="findScoreByStuNo" parameterType="java.lang.String" resultType="cn.wilfredshen.hnu.test.entity.Score">
        select * from score where stu_no = #{stuNo}
    </select>

</mapper>
